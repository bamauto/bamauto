\n\r\n// Supabase 에러는 'cause' 객체 안에 구체적인 정보가 담겨 있습니다.\r\nif (error.cause && error.cause.code) {\r\n    const code = error.cause.code;\r\n\r\n    switch (code) {\r\n        case 'invalid_credentials':\r\n            userMessage = \"이메일 또는 비밀번호가 올바르지 않습니다. 다시 확인해 주세요.\";\r\n            break;\r\n\r\n        case 'user_not_found':\r\n            userMessage = \"가입되지 않은 이메일 주소입니다. 회원가입을 먼저 진행해 주세요.\";\r\n            break;\r\n\r\n        case 'email_not_confirmed':\r\n            userMessage = \"이메일 인증이 완료되지 않았습니다. 메일함에서 인증 링크를 클릭하시거나, '인증 메일 재전송'을 요청해 주세요.\";\r\n            break;\r\n\r\n        case 'user_banned':\r\n            userMessage = \"이 계정은 접근이 제한되었습니다. 관리자에게 문의해 주세요.\";\r\n            break;\r\n\r\n        case 'over_request_rate_limit':\r\n            userMessage = \"로그인 시도 횟수를 초과했습니다. 잠시 후 다시 시도해 주세요.\";\r\n            break;\r\n\r\n        case 'validation_failed':\r\n            userMessage = \"이메일 또는 비밀번호의 형식이 올바르지 않습니다.\";\r\n            break;\r\n\r\n        // MFA(다중 인증) 관련 에러 처리\r\n        case 'mfa_verification_failed':\r\n            userMessage = \"인증 코드가 올바르지 않습니다. 다시 확인해 주세요.\";\r\n            break;\r\n\r\n        case 'mfa_challenge_expired':\r\n            userMessage = \"인증 코드의 유효 시간이 만료되었습니다. 다시 시도해 주세요.\";\r\n            break;\r\n\r\n        default:\r\n            // 위에 정의되지 않은 다른 Supabase 에러가 발생한 경우\r\n            console.error(\"Supabase Auth Error:\", error.cause); // 개발자 확인용 로그\r\n            userMessage = `로그인에 실패했습니다. 나중에 다시 시도해 주세요. (코드: ${code})`;\r\n    }\r\n} else if (error.message) {\r\n    // Supabase 에러가 아닌 일반적인 자바스크립트 에러의 경우\r\n    userMessage = error.message;\r\n}\r\n\r\n// 최종적으로 처리된 사용자 메시지를 반환합니다.\r\nreturn userMessage;\r\n","__wwtype":"js"}},"a99a1d94-4964-4158-975f-87bea0484e62":{"id":"a99a1d94-4964-4158-975f-87bea0484e62","args":{"provider":"google","redirectPage":"7a8974ae-710a-4119-a75d-ee2eaacaf6f4"},"type":"1fa0dd68-5069-436c-9a7d-3b54c340f1fa-signInProvider"}},"trigger":"click","firstAction":"a99a1d94-4964-4158