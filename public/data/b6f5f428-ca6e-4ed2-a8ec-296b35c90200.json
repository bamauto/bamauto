ontent.style.overflowY = 'auto';\n        content.style.width = '90%';\n        content.style.maxWidth = '100%';\n        \n        // Add iOS momentum scrolling\n        content.style.WebkitOverflowScrolling = 'touch';\n      }\n    });\n    \n    // Ensure popup content sections have proper spacing\n    const popupContentSections = wwLib.getFrontDocument().querySelectorAll('[data-weweb-id=\"575ec786-0d15-4502-bff1-cf94d31b4314\"], [data-weweb-id=\"7c1a2b37-339b-42e6-9599-a51cf07b85d7\"], [data-weweb-id=\"f30dda82-37d6-4189-923b-f16c96542aa9\"], [data-weweb-id=\"ec865b6b-a3fb-47e6-9798-71be00411fbc\"]');\n    popupContentSections.forEach(section => {\n      if (section) {\n        section.style.height = 'auto';\n        section.style.overflowY = 'visible';\n        section.style.padding = '16px';\n      }\n    });\n    \n    // Ensure form sections are properly spaced\n    const formSections = wwLib.getFrontDocument().querySelectorAll('[data-weweb-id=\"495b29be-570c-400f-a3fd-7fe1ec3f613b\"], [data-weweb-id=\"ca22dbaf-d24a-491c-b391-7651c19951c7\"]');\n    formSections.forEach(section => {\n      if (section) {\n        section.style.rowGap = '20px';\n      }\n    });\n    \n    // Add proper bottom padding to scrollable areas to ensure all content is accessible\n    const scrollableAreas = wwLib.getFrontDocument().querySelectorAll('[data-weweb-id=\"a3ea38ab-d370-4c12-ba49-44c9c4848a7e\"], [data-weweb-id=\"d2c5eb8a-f564-4ce5-bdea-501ece37e303\"], [data-weweb-id=\"faea991a-2b70-4b2a-9d0e-579609db6c4b\"], [data-weweb-id=\"4c268bba-6041-47be-afce-60d78287dbae\"]');\n    scrollableAreas.forEach(area => {\n      if (area) {\n        const lastChild = area.lastElementChild;\n        if (lastChild) {\n          lastChild.style.marginBottom = '16px';\n        }\n      }\n    });\n  }","next":null,"type":"custom-js"}},"firstAction":"9312f33f-fd26-44ed-9007-889dddfa5581","firstErrorAction":null},{"id":"5f8e1e21-2363-47fc-96dc-d9330d8d5632","parameters":[],"actions":{"0aefcd78-0393-4604-9615-faccf619118c":{"id":"0aefcd78-0393-4604-9615-faccf619118c","code":"try {\n    // Get current theme\n    const currentTheme = variables['0c3d472b-034c-46c8-a10c-8f874093b17f'];\n    \n    // Toggle theme\n    const newTheme = currentTheme === 'dark' ? 'light' : 'dark';\n    \n    // Update the theme variable\n    variables['0c3d472b-034c-46c8-a10c-8f874093b17f'] = newTheme;\n    \n    // Apply theme to document\n    const doc = wwLib.getFrontDocument();\n    if (doc && doc.documentElement) {\n      if (newTheme === 'dark') {\n        doc.documentElement.classList.add('ww-dark-theme');\n        doc.documentElement.classList.remove('ww-light-mode');\n      } else {\n        doc.documentElement.classList.remove('ww-dark-theme');\n        doc.documentElement.classList.add('ww-light-mode');\n      }\n    }\n    \n    // Store theme preference in localStorage\n    try {\n      wwLib.getFrontWindow().localStorage.setItem('theme', newTheme);\n    } catch (storageError) {\n      console.error('Failed to save theme preference to localStorage:', storageError);\n    }\n    \n    // Also update WeWeb's internal theme system\n    utilsFunctions.setTheme(newTheme);\n    \n    // Show a snackbar notification\n    variables['d67898b8-9a14-4a4c-aae7-35c8673c3c47'] = newTheme === 'dark' ? '다크 모드로 전환되었습니다' : '라이트 모드로 전환되었습니다';\n    variables['d0312ecc-4698-43d2-876a-2619f2a97984'] = true;\n    \n    // Auto-hide snackbar after 3 seconds\n    setTimeout(() => {\n      variables['d0312ecc-4698-43d2-876a-2619f2a97984'] = false;\n    }, 3000);\n    \n    return newTheme;\n  } catch (error) {\n    console.error('Error toggling theme:', error);\n    return variables['0c3d472b-034c-46c8-a10c-8f874093b17f']; // Return current theme if error\n  }","next":null,"type":"custom-js"}},"firstAction":"0aefcd78-0393-4604-9615-faccf619118c","firstErrorAction":null},{"id":"a0f41512-9114-41d4-8a36-bb3db7e18a6d","parameters":[{"name":"message","type":"string","defaultValue":""},{"name":"dur